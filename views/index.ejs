    <script>
        function responseHandler(res) {
            var flat_array = [];
            $.each(res, function(i, element) {
                flat_array.push(JSON.flatten(element));
            });
            return flat_array;
        }

    </script>

    <table id="data-table" class="table table-striped" data-url="../results.json" data-toggle="table" data-response-handler="responseHandler">
        <thead>
            <tr>
                <th data-field="table.concept" data-sortable="true">Entry Page</th>
                <th data-field="table.fileName" data-sortable="true">OS</th>
                <th data-field="table.URI" data-sortable="true">Conversion Rate</th>
            </tr>
        </thead>
    </table>
