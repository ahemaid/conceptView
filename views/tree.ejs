      // //show RDFType of the concept
      // showTypeFunc = function(event) {
      //   //event.stopPropagation();
      //   var conceptRDFType = [];
      //   for (var i = 0; i < viewDataRendered.length; i++) {
      //     if (event.id === viewDataRendered[i].concept) {
      //       conceptRDFType.push(viewDataRendered[i].RDFType);
